import AbraLib/types;

import AbraLib/halfadd;

template decFunc<T> [T] dec<T> (val [T]) = {
  halfSum = halfAdd<T>(val, -);
  return halfSum[1..];
};

use decFunc<Trit>;
use decFunc<Tryte>;
use decFunc<Tiny>;
use decFunc<Int>;
use decFunc<Huge>;
use decFunc<Hash>;
use decFunc<Hash3>;
use decFunc<Hash9>;
use decFunc<Signature>;
