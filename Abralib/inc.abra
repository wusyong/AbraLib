import AbraLib/types;

import AbraLib/halfadd;

template incFunc<T> [T] inc<T> (val [T]) = {
  halfSum = halfAdd<T>(val, 1);
  return halfSum[1..];
};

use incFunc<Trit>;
use incFunc<Tryte>;
use incFunc<Tiny>;
use incFunc<Int>;
use incFunc<Huge>;
use incFunc<Hash>;
use incFunc<Hash3>;
use incFunc<Hash9>;
use incFunc<Signature>;
